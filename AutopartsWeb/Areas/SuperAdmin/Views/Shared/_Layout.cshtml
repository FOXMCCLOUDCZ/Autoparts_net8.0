<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>SuperAdmin</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/SuperAdmin/assets/img/favicon.png" rel="icon">
    <link href="~/SuperAdmin/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/SuperAdmin/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/SuperAdmin/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/SuperAdmin/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/SuperAdmin/assets/vendor/quill/quill.snow.css" rel="stylesheet">
    <link href="~/SuperAdmin/assets/vendor/quill/quill.bubble.css" rel="stylesheet">
    <link href="~/SuperAdmin/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/SuperAdmin/assets/vendor/simple-datatables/style.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/SuperAdmin/assets/css/style.css" rel="stylesheet">
</head>


<body>
    <!-- === START Header === -->
    <header id="header" class="header fixed-top d-flex align-items-center">
        <!-- START logo -->
        <div class="d-flex align-items-center justify-content-between">
            <a asp-area="SuperAdmin" asp-controller="Dashboard" asp-action="Index" class="logo d-flex align-items-center">
                <img src="~/SuperAdmin/assets/img/logo.png" alt="">
                <span class="d-none d-lg-block">Unixcar</span>
            </a>
            <i class="bi bi-list toggle-sidebar-btn"></i>
        </div>
        <!-- END logo -->
    </header>
    <!-- === END Header === -->
    <!-- ============================== -->
    <!-- === START Sidebar === -->
    <aside id="sidebar" class="sidebar">
        <ul class="sidebar-nav" id="sidebar-nav">
            <!-- START Dashboard navigation -->
            <li class="nav-item">
                <a class="nav-link " href="index.html">
                    <i class="bi bi-grid"></i>
                    <span>Dashboard</span>
                </a>
            </li>
            <!-- END Dashboard navigation -->
            <!-- START xxx navigation -->
            <li class="nav-item">
                <a class="nav-link collapsed" href="#">
                    <i class="bi bi-gem"></i>
                    <span>About</span>
                </a>
            </li>
            <!-- END xxx navigation -->
            <!-- ============================== -->
            <!-- === START sekce nastaveni === -->
            <li class="nav-heading">Nastavení</li>
            <!-- START nastaveni uzivatelu -->
            <li class="nav-item">
                <a class="nav-link collapsed" href="#">
                    <i class="bi bi-gear"></i>
                    <span>Uživatelé</span>
                </a>
            </li>
            <!-- END nastaveni uzivatelu -->
            <!-- START nastaveni Webove stranky -->
            <li class="nav-item">
                <a class="nav-link collapsed" data-bs-target="#forms-nav" data-bs-toggle="collapse" href="#">
                    <i class="bi bi-gear"></i>
                    <span>Webové stránky</span>
                    <i class="bi bi-chevron-down ms-auto"></i>
                </a>
                <ul id="forms-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li>
                        <a href="forms-elements.html">
                            <i class="bi bi-circle"></i>
                            <span>Úvodní stránka</span>
                        </a>
                    </li>
                    <li>
                        <a asp-area="SuperAdmin" asp-controller="About" asp-action="GetAboutList">
                            <i class="bi bi-circle"></i>
                            <span>O nás</span>
                        </a>
                    </li>
                    <li>
                        <a asp-area="SuperAdmin" asp-controller="Contact" asp-action="GetContactList">
                            <i class="bi bi-circle"></i>
                            <span>Kontakt</span>
                        </a>
                    </li>
                    <li>
                        <a href="forms-validation.html">
                            <i class="bi bi-circle"></i>
                            <span>Naše služby</span>
                        </a>
                    </li>
                    <li>
                        <a href="forms-validation.html">
                            <i class="bi bi-circle"></i>
                            <span>Sociální sítě</span>
                        </a>
                    </li>
                    <li>
                        <a href="forms-validation.html">
                            <i class="bi bi-circle"></i>
                            <span>Team</span>
                        </a>
                    </li>
                    <li>
                        <a href="forms-validation.html">
                            <i class="bi bi-circle"></i>
                            <span>Recenze</span>
                        </a>
                    </li>
                </ul>
            </li>
            <!-- END nastaveni Webove stranky -->
            <!-- START nastaveni DATA -->
            <li class="nav-item">
                <a class="nav-link collapsed" data-bs-target="#tables-nav" data-bs-toggle="collapse" href="#">
                    <i class="bi bi-database"></i>
                    <span>Databáze (základní data)</span>
                    <i class="bi bi-chevron-down ms-auto"></i>
                </a>
                <ul id="tables-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li>
                        <a asp-area="SuperAdmin" asp-controller="" asp-action="">
                            <i class="bi bi-circle"></i>
                            <span>Nový</span>
                        </a>
                    </li>
                    <li>
                        <a asp-area="SuperAdmin" asp-controller="Brand" asp-action="BrandList">
                            <i class="bi bi-circle"></i>
                            <span>Brand</span>
                        </a>
                    </li>
                    <li>
                        <a asp-area="SuperAdmin" asp-controller="Currency" asp-action="CurrencyList">
                            <i class="bi bi-circle"></i>
                            <span>Měna</span>
                        </a>
                    </li>
                    <li>
                        <a asp-area="SuperAdmin" asp-controller="Warranty" asp-action="WarrantyList">
                            <i class="bi bi-circle"></i>
                            <span>Záruční lhůty</span>
                        </a>
                    </li>
                    <li>
                        <a asp-area="SuperAdmin" asp-controller="Country" asp-action="CountryList">
                            <i class="bi bi-circle"></i>
                            <span>Země</span>
                        </a>
                    </li>
                </ul>
            </li>
            <!-- END nastaveni DATA -->
        </ul>
    </aside>
    <!-- === END Sidebar === -->

    <main id="main" class="main">

        @RenderBody()

    </main>

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/SuperAdmin/assets/vendor/apexcharts/apexcharts.min.js"></script>
    <script src="~/SuperAdmin/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/SuperAdmin/assets/vendor/chart.js/chart.umd.js"></script>
    <script src="~/SuperAdmin/assets/vendor/echarts/echarts.min.js"></script>
    <script src="~/SuperAdmin/assets/vendor/quill/quill.min.js"></script>
    <script src="~/SuperAdmin/assets/vendor/simple-datatables/simple-datatables.js"></script>
    <script src="~/SuperAdmin/assets/vendor/tinymce/tinymce.min.js"></script>
    <script src="~/SuperAdmin/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/SuperAdmin/assets/js/main.js"></script>

</body>
</html>